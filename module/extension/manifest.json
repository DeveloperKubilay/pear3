{
    "manifest_version": 3,
    "name": "PearSystem",
    "version": "1.0",
    "description": "",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
        "cookies",
        "tabs"
    ]
}