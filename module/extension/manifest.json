{
    "manifest_version": 3,
    "name": "PearSystem",
    "version": "1.0",
    "description": "",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "content_security_policy": {
        "extension_pages": "default-src 'self'; style-src 'self'"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "cookies",
        "tabs"
    ]
}